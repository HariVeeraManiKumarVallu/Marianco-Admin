{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "shortDescription": {
      "type": "string"
    },
    "productCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "isArchived": {
      "type": "boolean",
      "default": false,
      "private": true
    },
    "popularity": {
      "type": "integer",
      "min": 0
    },
    "productId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "basePrice": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "skus": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sku.sku",
      "mappedBy": "product"
    },
    "variants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-variant.product-variant",
      "inversedBy": "products"
    },
    "optionValues": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-option-value.product-option-value",
      "inversedBy": "products"
    },
    "optionTypes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-option-type.product-option-type",
      "inversedBy": "products"
    },
    "description": {
      "type": "blocks"
    }
  }
}
