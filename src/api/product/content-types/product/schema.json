{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "product_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "isArchived": {
      "type": "boolean",
      "default": false,
      "private": true
    },
    "popularity": {
      "type": "integer",
      "min": 0
    },
    "variants": {
      "type": "json"
    },
    "images": {
      "type": "json"
    },
    "supplierProductId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "basePrice": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "mdDescription": {
      "type": "richtext"
    },
    "longDescription": {
      "type": "blocks"
    }
  }
}
