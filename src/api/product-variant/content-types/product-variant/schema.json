{
  "kind": "collectionType",
  "collectionName": "product_variants",
  "info": {
    "singularName": "product-variant",
    "pluralName": "product-variants",
    "displayName": "Product Variant",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "options": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-option-value.product-option-value",
      "mappedBy": "variants"
    },
    "skus": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sku.sku",
      "mappedBy": "variant"
    },
    "cost": {
      "type": "integer",
      "min": 0,
      "private": true
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "weight": {
      "type": "integer",
      "min": 0
    },
    "isEnabled": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "isDefault": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "isAvailable": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "variants"
    },
    "variantId": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "images": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-image.product-image",
      "inversedBy": "variants"
    }
  }
}
