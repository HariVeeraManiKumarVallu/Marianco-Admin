{
  "kind": "collectionType",
  "collectionName": "product_options",
  "info": {
    "singularName": "product-option",
    "pluralName": "product-options",
    "displayName": "Product Option",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "color",
        "size",
        "paper"
      ],
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "optionId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "product_variants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-variant.product-variant",
      "mappedBy": "options"
    }
  }
}
