{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "orderNumber": {
      "type": "integer",
      "unique": true,
      "required": false,
      "min": 100000
    },
    "totalPrice": {
      "type": "decimal",
      "required": true
    },
    "isPaid": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "orderItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "order"
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "SEK",
        "USD",
        "EUR"
      ],
      "default": "USD",
      "required": true
    },
    "stripePaymentId": {
      "type": "string",
      "private": true
    },
    "shippingCost": {
      "type": "integer"
    },
    "customer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::customer.customer",
      "mappedBy": "order"
    }
  }
}
